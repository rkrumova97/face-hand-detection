<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="585.0" prefWidth="768.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controller.SceneController">
    <children>
        <CheckBox fx:id="haarClassifier" layoutX="147.0" layoutY="25.0" mnemonicParsing="false" text="Haar Classifier"/>
        <CheckBox fx:id="lbpClassifier" layoutX="421.0" layoutY="25.0" mnemonicParsing="false" text="LBP Classifier"/>
        <Separator layoutX="3.0" layoutY="56.0" prefHeight="13.0" prefWidth="768.0"/>
        <ImageView fx:id="originalFrame" fitHeight="345.0" fitWidth="498.0" layoutX="137.0" layoutY="132.0"
                   pickOnBounds="true" preserveRatio="true"/>
        <Button fx:id="chooseImage" layoutX="224.0" layoutY="532.0" mnemonicParsing="false" prefHeight="39.0"
                prefWidth="124.0" text="Choose image"/>
        <Button fx:id="cameraButton" layoutX="554.0" layoutY="532.0" mnemonicParsing="false" text="Start camera"/>
        <CheckBox fx:id="fishingAlgorithm" layoutX="77.0" layoutY="81.0" mnemonicParsing="false"
                  text="Fishing algorithm"/>
        <CheckBox fx:id="eigenFacesAlgorithm" layoutX="293.0" layoutY="81.0" mnemonicParsing="false"
                  text="Eigenfaces "/>
        <CheckBox fx:id="lbphAlgorithm" layoutX="469.0" layoutY="81.0" mnemonicParsing="false"
                  text="Local Binary Patterns Histograms"/>
        <Separator layoutX="2.0" layoutY="119.0" prefHeight="13.0" prefWidth="768.0"/>
        <Button fx:id="train" layoutX="396.0" layoutY="532.0" mnemonicParsing="false" prefHeight="39.0"
                prefWidth="124.0" text="Train algorithm"/>
        <Button fx:id="predict" layoutX="63.0" layoutY="532.0" mnemonicParsing="false" prefHeight="39.0"
                prefWidth="124.0" text="Predict"/>
        <Label fx:id="label" layoutX="365.0" layoutY="483.0" text="Label"/>
    </children>
</AnchorPane>
